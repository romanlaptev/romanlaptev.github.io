<?xml version="1.0" encoding="utf-8"?>
<templates>


	<tpl name="tpl-videolist">
		<html_code><![CDATA[
<div class="panel">{{list}}</div>
		]]></html_code>
	</tpl>

	<tpl name="tpl-videolist-item--video">
		<html_code>
<![CDATA[
<div class="panel-body {{type}}">
	<div>{{title}}</div>
	
	<div>{{producer}}</div>
	<div class="roles">{{roles}}</div>

	<div>{{published}}</div>
	<div>{{updated}}</div>

	<div>{{pictures}}</div>
	<div class="mark description">{{description}}</div>

	<div>{{tags}}</div>
	<div>{{ul}}</div>
</div>	
	
]]>
		</html_code>
	</tpl>
	
	<tpl name="tpl-videolist-item--videoclip">
		<html_code>
<![CDATA[
<div class="panel-body {{type}}">
	<div>{{title}}</div>

	<div>{{creators}}</div>
​​​​​​
	<div>{{published}}</div>
	<div>{{updated}}</div>

	<div>{{pictures}}</div>
	<div class="mark description">{{description}}</div>

	<div>{{tags}}</div>
	<div>{{ul}}</div>
</div>	
]]>
		</html_code>
	</tpl>

	<tpl name="tpl-videolist-item--title">
		<html_code>
<![CDATA[
	<h3>{{text}}</h3>
]]>
		</html_code>
	</tpl>



	<tpl name="tpl-videolist-list-links">
		<html_code><![CDATA[
<ul class="">{{list}}</ul>
		]]></html_code>
	</tpl>
	
	<tpl name="tpl-videolist-item--ul">
		<html_code>
<![CDATA[
	<li><a data-type="{{data-type}}" href="{{href}}" target="_blank">{{text}}</a></li>
]]>
		</html_code>
	</tpl>
	

	<tpl name="tpl-videolist-list-tags">
		<html_code><![CDATA[
<ul class="list-inline">{{list}}</ul>
		]]></html_code>
	</tpl>
	
	<tpl name="tpl-videolist-item--tag">
		<html_code>
<![CDATA[
	<li><a data-tid="{{tid}}" href="#">{{text}}</a></li>
]]>
		</html_code>
	</tpl>


	<tpl name="tpl-videolist-list-pictures">
		<html_code><![CDATA[
<ul class="">{{list}}</ul>
		]]></html_code>
	</tpl>
	
	<tpl name="tpl-videolist-item--img">
		<html_code>
<![CDATA[
	<li><img src="{{src}}" title="{{src}}" alt="{{src}}"/></li>
]]>
		</html_code>
	</tpl>





	<tpl name="tpl-block">
		<html_code><![CDATA[
<div class="block panel panel-primary">
	<div class="panel-heading">
		<h3 class="text-center">{{block_title}}</h3>
	</div>
	<div class="panel-body">
		<div class="content">{{content}}</div>
	</div>
</div>			
		]]></html_code>
	</tpl>	
	
	<tpl name="tpl-block-videolist">
		<html_code><![CDATA[
<div class="block panel">
	<div class="panel-body">
		<div class="content">{{content}}</div>
	</div>
</div>			
		]]></html_code>
	</tpl>	
