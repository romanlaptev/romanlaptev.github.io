	<tpl name="tpl-videolist-item--ul">
		<html_code>
<![CDATA[
	<li>
		<!-- <a class="btn btn-lg btn-orange" data-type="{{data-type}}" href="{{href}}" target="_blank">{{data-type}}, {{text}}</a> -->
<div class="panel-body">
	<div class="">
		<b>{{text}}</b>, <i>{{data-type}}</i>
	</div>
	<div class="">
		<button type="button" class="btn btn-lg btn-orange {{class_support}}" value="{{href}}" data-type="{{data-type}}">open video in new tab</button>
